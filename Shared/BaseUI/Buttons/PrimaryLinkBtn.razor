@namespace Embyte.Shared.BaseUI.Buttons


<a href='@(!disabled ? link : "")' target="@Target">
    <PrimaryBtn t=@t disabled=@disabled />
</a>

@code {
    [Parameter]
    public string link { get; set; } = default!;
    [Parameter]
    public string t { get; set; } = "Continue";
    [Parameter]
    public bool disabled { get; set; } = false;
    [Parameter]
    public string Target { get; set; } = "_blank";
}